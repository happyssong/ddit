import java.util.Scanner;

public class MultiLoop4 {

	public static void main(String[] args) {
		//구구단 수를 입력
		Scanner scr=new Scanner(System.in);
		
		//입력
		while(true) {
			System.out.println("수를 입력 : ");
			int x = scr.nextInt();	
		
			if(x==0) {
				break;	
			}//문장 1개라 중괄호 없어도 됨
			
			for(int i=1; i<=9; i++) {
				System.out.println(x+"*"+i+"="+x*i);
			}
			//종료조건이 여기있으면 0단까지 출력됨
		}
		
		
		
	}

}
